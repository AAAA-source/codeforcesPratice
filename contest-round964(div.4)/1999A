想法：直接做即可

#include<iostream>
#include<vector>
#include<string>
#include<algorithm>

using std::cin ;
using std::cout ;
using std::endl ;
using std::vector ;
using std::string ;



int main(void)
{
    int t ;
    cin >> t ;
    
    while (t--) {
        string s ;
        cin >> s ;
        
        cout << s[0] - '0' + s[1] - '0' << endl ;
    }
    
    
    return 0 ;
}
