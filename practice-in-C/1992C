想法：創造一個n , n - 1 , ... , k , k - 1 , ... , m + 1 , 1 , 2 , 3 , ... , m 的陣列
就可以使題目要求的數值最大

#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<stdbool.h>
#include<limits.h>

int main(void)
{
    int t ;
    scanf("%d" , &t) ;
    
    while (t--) {
        int n , m , k ;
        scanf("%d%d%d" , &n , &m , &k) ;
        
        int array[n] ;
        for(int i = 0 ; i < n - k + 1 ; i++)
            array[i] = n - i ;
        for(int i = 0 ; i < k - m - 1 ; i++)
            array[n - k + 1 + i] = k - 1 - i ;
        for(int i = 0 ; i < m ; i++)
            array[n - m + i] = i + 1 ;
            
        for(int i = 0 ; i < n ; i++)
            printf("%d " , array[i]) ;
        putchar('\n') ;
    }
    
    
    
    return 0 ;
}
