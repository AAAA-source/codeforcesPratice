想法：只要到第一位是 0 ，其餘是 1 的那個數即可
也就是01...1

#include<iostream>
#include<vector>
#include<string>
#include<algorithm>

using std::cin ;
using std::cout ;
using std::endl ;
using std::vector ;
using std::string ;


int main (void)
{
    int t ;
    cin >> t ;
    
    while (t--) {
        long long int n ;
        cin >> n ;
        
        long long int now = 1 ;
        while ( n >= now ) {
            now *= 2 ;
        }
        
        
        cout << now / 2 - 1 << endl ;
    }
    
    
    
    return 0 ;
}
