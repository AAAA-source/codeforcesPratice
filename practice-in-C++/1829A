想法：逐字比較即可

#include<iostream>
#include<vector>
#include<string>

using std::cin ;
using std::cout ;
using std::endl ;
using std::vector ;
using std::string ;

int main(void)
{
    int t ;
    cin >> t ;
    
    
    string compare = "codeforces" ;
    while (t--) {
        string s ;
        cin >> s ;
        
        int count = 0 ;
        for( decltype(s.size()) i = 0 ; i < s.size() ; i++ ) {
            if ( compare[i] != s[i] )
                count++ ;
        }
        
        
        
        cout << count << endl ;
    }
    
    
    return 0 ;
}
